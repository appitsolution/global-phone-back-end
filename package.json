{
  "name": "payload-template-js-blank",
  "description": "Payload project created from js-blank template",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "copyfiles": "copyfiles -u 1 \"./**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=./payload.config.js payload build",
    "dev": "nodemon",
    "build": "yarn copyfiles && yarn build:payload",
    "serve": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@payloadcms/plugin-form-builder": "^1.0.13",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-seo": "^1.0.10",
    "axios": "^1.2.3",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "build": "^0.1.4",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "jwt": "^0.2.0",
    "mongoose": "^6.8.1",
    "node-cron": "^3.0.2",
    "payload": "^1.5.9",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
